package com.jy.entiy.function.marketingFunction.lotteryManagement;

public class Drawlotteryitem {
	//奖项表自增长主键
    private Integer drawlotteryitemid;
    //抽奖配置表自增长字段
    private Integer drawlotteryconfigid;
    //奖项设置(奖品名称)
    private String lotteryset;
    //奖品数量
    private Integer lotteryno;
    //中奖概率
    private Float getlotteryprobalility;
    //奖项等级id
    private Integer drawlotterylevelid;
    //图片地址id(文件表自增长主键)
    private Long picturepathid;
    //图片地址
    private String picturepath;
    //前端使用虚拟状态
    private Integer status;
    public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public Integer getDrawlotteryitemid() {
        return drawlotteryitemid;
    }

    public void setDrawlotteryitemid(Integer drawlotteryitemid) {
        this.drawlotteryitemid = drawlotteryitemid;
    }

    public Integer getDrawlotteryconfigid() {
        return drawlotteryconfigid;
    }

    public void setDrawlotteryconfigid(Integer drawlotteryconfigid) {
        this.drawlotteryconfigid = drawlotteryconfigid;
    }

    public String getLotteryset() {
        return lotteryset;
    }

    public void setLotteryset(String lotteryset) {
        this.lotteryset = lotteryset == null ? null : lotteryset.trim();
    }

    public Integer getLotteryno() {
        return lotteryno;
    }

    public void setLotteryno(Integer lotteryno) {
        this.lotteryno = lotteryno;
    }

    public Float getGetlotteryprobalility() {
        return getlotteryprobalility;
    }

    public void setGetlotteryprobalility(Float getlotteryprobalility) {
        this.getlotteryprobalility = getlotteryprobalility;
    }

    public Integer getDrawlotterylevelid() {
        return drawlotterylevelid;
    }

    public void setDrawlotterylevelid(Integer drawlotterylevelid) {
        this.drawlotterylevelid = drawlotterylevelid;
    }

    public Long getPicturepathid() {
        return picturepathid;
    }

    public void setPicturepathid(Long picturepathid) {
        this.picturepathid = picturepathid;
    }

    public String getPicturepath() {
        return picturepath;
    }

    public void setPicturepath(String picturepath) {
        this.picturepath = picturepath == null ? null : picturepath.trim();
    }
}