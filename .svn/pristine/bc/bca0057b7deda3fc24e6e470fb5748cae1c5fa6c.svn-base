package com.jy.entiy.function.marketingFunction.lotteryManagement;

import java.util.Date;

/**
 * 抽奖记录实体类
 * @author chenye
 *
 */
public class Persondrawlottery extends PersondrawlotteryKey {
	
    private String remark;
    //中奖时间
    private Date winningtime;    
    //标题
    private String drawlotterytitle;
    //中奖人姓名
    private String name;
    //奖项设置(奖品名称)
    private String lotteryset;   
    //奖项级别
    private String DrawLotteryLevelName;
    
    
    
    

    /**
	 * @return the drawLotteryLevelName
	 */
	public String getDrawLotteryLevelName() {
		return DrawLotteryLevelName;
	}

	/**
	 * @param drawLotteryLevelName the drawLotteryLevelName to set
	 */
	public void setDrawLotteryLevelName(String drawLotteryLevelName) {
		DrawLotteryLevelName = drawLotteryLevelName;
	}

	/**
	 * @return the drawlotterytitle
	 */
	public String getDrawlotterytitle() {
		return drawlotterytitle;
	}

	/**
	 * @param drawlotterytitle the drawlotterytitle to set
	 */
	public void setDrawlotterytitle(String drawlotterytitle) {
		this.drawlotterytitle = drawlotterytitle;
	}

	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}

	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}

	/**
	 * @return the lotteryset
	 */
	public String getLotteryset() {
		return lotteryset;
	}

	/**
	 * @param lotteryset the lotteryset to set
	 */
	public void setLotteryset(String lotteryset) {
		this.lotteryset = lotteryset;
	}

	public String getRemark() {
        return remark;
    }

    public void setRemark(String remark) {
        this.remark = remark == null ? null : remark.trim();
    }

    public Date getWinningtime() {
        return winningtime;
    }

    public void setWinningtime(Date winningtime) {
        this.winningtime = winningtime;
    }
}